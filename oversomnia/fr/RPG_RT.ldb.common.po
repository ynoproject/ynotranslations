msgid ""
"\\S[1]Effects Owned: \\V[3]/15\\.\n"
"Menus Unlocked: \\V[15]/6\\.\n"
"Times Slept: \\V[17]"
msgstr ""
"\\S[1]Effets possédés : \\V[3]/15\\.\n"
"Menus débloqués : \\V[15]/6\\.\n"
"Nombre de fois endormi : \\V[17]"

#. ID 28, Line 5
msgid ""
"\\S[1]Vending Machines Used: \\V[38]\\.\n"
"Total Fish Caught: \\V[52]"
msgstr ""
