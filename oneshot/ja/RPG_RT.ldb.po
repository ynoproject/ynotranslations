#. ID 1
msgctxt "items.name"
msgid "lightbulb"
msgstr "電球"

#. ID 1
msgctxt "items.description"
msgid "A giant, yellow lightbulb. Where did it come from?"
msgstr "巨大な、黄色い電球。どこから来たのか？"

#. ID 2
msgctxt "items.name"
msgid "television remote"
msgstr "テレビのリモコン"

#. ID 2
msgctxt "items.description"
msgid "It's too hard to read in the dark light."
msgstr "暗い場所では文字が読めない。"

#. ID 3
msgctxt "items.name"
msgid "bottle of alcohol"
msgstr "アルコールのビン"

#. ID 3
msgctxt "items.description"
msgid "The smell of the liquid is repugnant."
msgstr "液体は不快な臭いがする。"

#. ID 4
msgctxt "items.name"
msgid "dry branch"
msgstr "乾いた枝"

#. ID 4
msgctxt "items.description"
msgid "A flaky, dry branch from the bathroom plant."
msgstr "浴室にある観葉植物の、パサパサに乾いた枝。"

#. ID 5
msgctxt "items.name"
msgid "wet branch"
msgstr "濡れた枝"

#. ID 5
msgctxt "items.description"
msgid "The tip of the branch is stained with alcohol."
msgstr "枝の先はアルコールで濡れている。"

#. ID 6
msgctxt "items.name"
msgid "torch"
msgstr "たいまつ"

#. ID 6
msgctxt "items.description"
msgid "An improvised torch. Doesn't give off much light."
msgstr "即席のたいまつ。あまり明るくない。"

#. ID 7
msgctxt "items.name"
msgid "basement key"
msgstr "地下室の鍵"

#. ID 7
msgctxt "items.description"
msgid "This thing looks old."
msgstr "なんだか古めかしい。"

#. ID 8
msgctxt "items.name"
msgid "camera"
msgstr "カメラ"

#. ID 8
msgctxt "items.description"
msgid "A broken camera featuring a large screwed-in lens."
msgstr "大きなねじ込み式レンズが特徴の、壊れたカメラ。"

#. ID 9
msgctxt "items.name"
msgid "screwdriver"
msgstr "ドライバー"

#. ID 9
msgctxt "items.description"
msgid "Phillips-head."
msgstr "プラスのネジに対応。"

#. ID 10
msgctxt "items.name"
msgid "lens"
msgstr "レンズ"

#. ID 10
msgctxt "items.description"
msgid "A flawless, large lens."
msgstr "曇り一つない、大きなレンズ。"

#. ID 11
msgctxt "items.name"
msgid "empty bottle"
msgstr "空ビン"

#. ID 11
msgctxt "items.description"
msgid "The smell of alcohol lingers."
msgstr "アルコールの臭いが残っている。"

#. ID 12
msgctxt "items.name"
msgid "broken battery"
msgstr "壊れたバッテリー"

#. ID 12
msgctxt "items.description"
msgid "The lens is completely shattered."
msgstr "レンズは完全に粉々だ。"

#. ID 13
msgctxt "items.name"
msgid "empty battery"
msgstr "空のバッテリー"

#. ID 13
msgctxt "items.description"
msgid "A working, rechargable battery. Currently empty."
msgstr "作動可能な充電式バッテリー。今は充電されてない。"

#. ID 14
msgctxt "items.name"
msgid "charged battery"
msgstr "充電済みバッテリー"

#. ID 14
msgctxt "items.description"
msgid "A working, rechargable battery. Maximum power."
msgstr "作動可能な充電式バッテリー。フルチャージ。"

#. ID 15
msgctxt "items.name"
msgid "bottle of smoke"
msgstr "煙入りビン"

#. ID 15
msgctxt "items.description"
msgid "It's full of noxious fumes."
msgstr "有毒ガスでいっぱいだ。"

#. ID 16
msgctxt "items.name"
msgid "bottle of acid"
msgstr "酸の入ったビン"

#. ID 16
msgctxt "items.description"
msgid "It's full of a thick liquid acid."
msgstr "粘っこい酸性の液体でいっぱいだ。"

#. ID 17
msgctxt "items.name"
msgid "wet sponge"
msgstr "濡れたスポンジ"

#. ID 17
msgctxt "items.description"
msgid "Sponge soaked with acidic cleaning solution."
msgstr "酸性の洗浄液が染み込んだスポンジ。"

#. ID 18
msgctxt "items.name"
msgid "crowbar"
msgstr "バールのようなもの"

#. ID 18
msgctxt "items.description"
msgid "Makeshift crowbar made from a pipe."
msgstr "パイプから作った、間に合わせのバールのようなもの。"

#. ID 19
msgctxt "items.name"
msgid "metal rod"
msgstr "鉄パイプ"

#. ID 19
msgctxt "items.description"
msgid "A long, slender metal bar."
msgstr "長くて細い金属の棒。"

#. ID 20
msgctxt "items.name"
msgid "sponge"
msgstr "スポンジ"

#. ID 20
msgctxt "items.description"
msgid "A flat sponge with a rough surface."
msgstr "表面が粗い平らなスポンジ。"

#. ID 21
msgctxt "items.name"
msgid "empty syringe"
msgstr "空の注射器"

#. ID 21
msgctxt "items.description"
msgid "The needle is particularly long."
msgstr "針が非常に長い。"

#. ID 22
msgctxt "items.name"
msgid "viscous syringe"
msgstr "粘液入り注射器"

#. ID 22
msgctxt "items.description"
msgid "It's full of the strange liquid from the pond."
msgstr "池の奇妙な液体で満たされている。"

#. ID 23
#. ID 50
msgctxt "items.name"
msgid "coin"
msgstr "コイン"

#. ID 23
msgctxt "items.description"
msgid "It has a three-leaf clover pattern."
msgstr "三つ葉のクローバーの模様がある。"

#. ID 24
msgctxt "items.name"
msgid "weird journal"
msgstr "変な日記"

#. ID 24
msgctxt "items.description"
msgid "It's written in an unknown language."
msgstr "謎の言語で書かれている。"

#. ID 25
#. ID 51
msgctxt "items.name"
msgid "feather"
msgstr "羽根"

#. ID 25
msgctxt "items.description"
msgid "A long and slender feather."
msgstr "長細い羽根。"

#. ID 26
msgctxt "items.name"
msgid "ink bottle"
msgstr "インクのビン"

#. ID 26
msgctxt "items.description"
msgid "Obtained from a trading post."
msgstr "交換所で入手した。"

#. ID 27
msgctxt "items.name"
msgid "miracle water"
msgstr "奇跡の水"

#. ID 27
msgctxt "items.description"
msgid "It's said to make plants grow very quickly."
msgstr "植物をとても速く成長させると書かれている。"

#. ID 28
msgctxt "items.name"
msgid "seed"
msgstr "種"

#. ID 28
msgctxt "items.description"
msgid "It's a seed apparently left behind by Maize."
msgstr "メイズが残していった種のようだ。"

#. ID 29
msgctxt "items.name"
msgid "wool"
msgstr "羊毛"

#. ID 29
msgctxt "items.description"
msgid "It's... kind of useless."
msgstr "あまり…役に立たない。"

#. ID 30
msgctxt "items.name"
msgid "pen"
msgstr "ペン"

#. ID 30
msgctxt "items.description"
msgid "A pen made from dipping a feather into ink."
msgstr "羽根をインクにつけて使うペン。"

#. ID 31
#. ID 52
msgctxt "items.name"
msgid "die"
msgstr "サイコロ"

#. ID 31
msgctxt "items.description"
msgid "A die received from George."
msgstr "ジョージから貰ったサイコロ。"

#. ID 50
#. ID 51
#. ID 52
msgctxt "items.description"
msgid "It's buzzing..."
msgstr "唸っている…"

#. ID 97
msgctxt "items.name"
msgid "novelty T-shirt"
msgstr "ノベルティTシャツ"

#. ID 97
msgctxt "items.description"
msgid "I herded rams and all I got was this lousy T-shirt"
msgstr "羊の世話をして手に入れたのはちゃっちいTシャツだけ"

#. ID 98
msgctxt "items.name"
msgid "boots"
msgstr "ブーツ"

#. ID 98
msgctxt "items.description"
msgid "These boots will make it possible to move quickly."
msgstr "このブーツがあれば、素早く動くことができる。"

#. ID 99
msgctxt "items.name"
msgid "rubber gloves"
msgstr "ゴム手袋"

#. ID 99
msgctxt "items.description"
msgid "Gloves designed to prevent chemical damage."
msgstr "化学物質による怪我を防ぐための手袋。"

#. ID 100
msgctxt "items.name"
msgid "gas mask"
msgstr "ガスマスク"

#. ID 100
msgctxt "items.description"
msgid "A mask used to filter air for breathing."
msgstr "息が吸えるよう、空気をフィルターするマスク。"

