msgid ""
"\\>かめさん\n"
"\\>うさぎさん (重い)\n"
"\\>このままでいいです"
msgstr ""
"\\>Медленно\n"
"\\>Быстро (Тяжелее)\n"
"\\>Оставить как есть"
